<!-- Template: {{ _self }}.twig -->



        <div class="mb-2"> {{ products|length }} von {{ pageInfo.total }} Artikeln.</div>
        {% if pageInfo.currentPage != 1 %}<a class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300"
                                            href="{{ pageInfo.firstUrl }}">Erste</a>{% endif %}
        {% if pageInfo.prevUrl %}<a class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300"
                                    href="{{ pageInfo.prevUrl }}">Previous</a>{% endif %}

        {% for page, url in pageInfo.getPrevUrls(5) %}
            <a class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300" href="{{ url }}">{{ page }}</a>
        {% endfor %}

        <span class="px-2 py-1">{{ pageInfo.currentPage }}</span>

        {% for page, url in pageInfo.getNextUrls(5) %}
            <a class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300" href="{{ url }}">{{ page }}</a>
        {% endfor %}

        {% if pageInfo.nextUrl %}<a class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300"
                                    href="{{ pageInfo.nextUrl }}">NÃ¤chste Seite</a>{% endif %}
        {% if pageInfo.currentPage != pageInfo.totalPages %}<a
            class="text-blue-400px-2 py-1 px-2 rounded border border-gray-300" href="{{ pageInfo.lastUrl }}">
            Letze</a>{% endif %}

