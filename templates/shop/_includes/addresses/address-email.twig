{% set boldKeys = ['attention', 'name', 'businessName'] %}
<!-- Template: {{ _self }}.twig -->
<div class="list">
  <ul class="list__adress-email">
    {% for key, line in address.addressLines %}
      <li>
        {% if key in boldKeys %}
          <strong>{% if key == 'attention' %}Attn:{% endif %}
        {% endif %}
        {{ line }}
        {% if key in boldKeys %}
          </strong>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</div>
