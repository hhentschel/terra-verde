{
  "require": {
    "craftcms/cms": "^3.7.20",
    "craftcms/commerce": "^3.4.7",
    "craftcms/feed-me": "^4.4.0",
    "craftcms/redactor": "^2.8.8",
    "doublesecretagency/craft-googlemaps": "^4.0.11",
    "kuriousagency/commerce-bundles": "^1.2.1",
    "mediabeastnz/craft-commerce-back-in-stock": "^1.4.0",
    "nystudio107/craft-imageoptimize": "^1.6",
    "nystudio107/craft-retour": "^3.1.65",
    "pompom/commerce-datatrans": "^1.0",
    "pompom/omnipay-datatrans": "^3.0",
    "putyourlightson/craft-sendgrid": "^1.2.2",
    "putyourlightson/craft-sprig": "^1.10.4",
    "spicyweb/craft-neo": "^2.11.15",
    "verbb/expanded-singles": "^1.1.4",
    "verbb/formie": "^1.4.23",
    "vlucas/phpdotenv": "^3.4.0",
    "pompom/address-importer": "@dev"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\terraverdemodule\\": "modules/terraverdemodule/src/",
      "modules\\exampleadjuster\\": "modules/exampleadjuster/src/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.12"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "omnipay-datatrans": {
      "type": "vcs",
      "url": "git@bitbucket.org:pom-pom/omnipay-datatrans.git"
    },
    "commerce-datatrans": {
      "type": "vcs",
      "url": "git@bitbucket.org:pom-pom/commerce-datatrans.git"
    },
    "craft-user-guide": {
      "type": "path",
      "url": "./plugins/*"
    },
    "address-importer": {
      "type": "path",
      "url": "./craft-plugins/address-importer/",
      "options": {
        "symlink": true
      }
    }
  }
}
