{
  "require": {
    "craftcms/cms": "^3.5.10",
    "craftcms/commerce": "3.2.17.4",
    "craftcms/feed-me": "4.3.6",
    "craftcms/redactor": "2.8.5",
    "doublesecretagency/craft-googlemaps": "4.0.5",
    "kuriousagency/commerce-bundles": "1.2.1",
    "mediabeastnz/craft-commerce-back-in-stock": "1.4.0",
    "putyourlightson/craft-sprig": "1.5.2",
    "spicyweb/craft-neo": "2.9.5",
    "verbb/expanded-singles": "1.1.3",
    "verbb/formie": "1.3.27",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4.12"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
